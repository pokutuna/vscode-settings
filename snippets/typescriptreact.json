{
  // Place your snippets for typescriptreact here. Each snippet is defined under a snippet name and has a prefix, body and
  // description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
  // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the
  // same ids are connected.
  // Example:
  // "Print to console": {
  // 	"prefix": "log",
  // 	"body": [
  // 		"console.log('$1');",
  // 		"$2"
  // 	],
  // 	"description": "Log output to console"
  // }

  "whyDidYouUpdate": {
    "prefix": "whyDidYouUpdate",
    "body": [
      "import { whyDidYouUpdate } from 'why-did-you-update';",
      "whyDidYouUpdate(React, { include: /pattern/, exclude: /pattern/});"
    ]
  },

  "eslint-disable-line": {
    "prefix": "ignore-inline-eslint",
    "body": ["// eslint-disable-line ${1:<...rule>}"]
  },
  "eslint-disable-next-line": {
    "prefix": "ignore-nextline-eslint",
    "body": ["// eslint-disable-next-line ${1:<...rule>}"]
  },
  "eslint-disable": {
    "prefix": "ignore-range",
    "body": [
      "/* eslint-disable {$1:<...rule>} */",
      "/* eslint-enable {$1:<...rule>} */"
    ]
  }
}
