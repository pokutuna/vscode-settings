{
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "window.zoomLevel": 0,
  /**
   Global editor.*
   */
  "editor.fontFamily": "'VL Gothic', Menlo, Monaco, 'Courier New', monospace",
  // 'VL Gothic', 'JetBrains Mono', 'Fira Code', 'Ricty Diminished Discord'
  "editor.fontSize": 13,
  //"editor.lineHeight": 16,
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.autoClosingBrackets": "always",
  "editor.tabSize": 2,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "window.restoreWindows": "none",
  "workbench.editor.showTabs": false,
  "breadcrumbs.enabled": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  /** Languages */
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "September Steel",
  "editor.renderWhitespace": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[go]": {
    "editor.formatOnSave": true
  },
  "go.useLanguageServer": true,
  "[terraform]": {
    "editor.formatOnSave": true
  },
  "terraform.languageServer": {
    "external": true,
    "args": [
      "serve"
    ]
  },
  /** Extensions */
  "liveshare.featureSet": "insiders",
  "InstantWeather.location": "kyoto",
  "InstantWeather.key": "669a4c0cede7b56ea305802c1ae89dca",
  "clipring.maxRingItems": 20,
  "docker.truncateMaxLength": 30,
  "todo-tree.highlights.defaultHighlight": {},
  "todo-tree.tree.showScanModeButton": false,
  "git.autofetch": true,
}
