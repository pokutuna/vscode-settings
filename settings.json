{
  "window.zoomLevel": 0,
  "editor.fontFamily": "'VL Gothic', Menlo, Monaco, 'Courier New', monospace",
  "editor.minimap.enabled": false,
  "editor.autoClosingBrackets": "always",
  "breadcrumbs.enabled": true,
  "editor.fontSize": 13,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.tabSize": 2,
  "[typescript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[markdown]": {
    "files.trimTrailingWhitespace": false,
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "September Steel",
  "InstantWeather.location": "kyoto",
  "InstantWeather.key": "669a4c0cede7b56ea305802c1ae89dca",
  "clipring.maxRingItems": 20,
  "docker.truncateMaxLength": 30,
  "editor.renderWhitespace": "none",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.autoFixOnSave": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveshare.featureSet": "insiders",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[go]": {
    "editor.formatOnSave": true
  },
  "go.useLanguageServer": true,
  "todo-tree.highlights.defaultHighlight": {},
  "git.autofetch": true
}
